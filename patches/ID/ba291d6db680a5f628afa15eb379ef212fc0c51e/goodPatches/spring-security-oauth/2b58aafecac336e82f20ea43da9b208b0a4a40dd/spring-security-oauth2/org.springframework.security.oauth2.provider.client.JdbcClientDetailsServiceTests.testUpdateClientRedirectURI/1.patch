 	service.addClientDetails(clientDetails); 
  
 	String[] redirectURI = { "http://localhost:8080", 
 			"http://localhost:9090" }; 
 	clientDetails.setRegisteredRedirectUri(new LinkedHashSet<String>(Arrays 
 			.asList(redirectURI))); 
  
